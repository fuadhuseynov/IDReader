INC_RELEASEDIST = $(INC) -I../../../Include
CFLAGS_RELEASEDIST = $(CFLAGS) -O2
RESINC_RELEASEDIST = $(RESINC)
RCFLAGS_RELEASEDIST = $(RCFLAGS)
LIBDIR_RELEASEDIST = $(LIBDIR) -L../../../../Dist
LIB_RELEASEDIST = $(LIB)
LDFLAGS_RELEASEDIST = $(LDFLAGS) -s
OBJDIR_RELEASEDIST = obj/Release
DEP_RELEASEDIST = 
OUT_RELEASEDIST = bin/Release/SwipeCmdPrompt

OBJ_RELEASEDIST = $(OBJDIR_RELEASEDIST)/Main.o $(OBJDIR_RELEASEDIST)/SwipeCmdPrompt.o $(OBJDIR_RELEASEDIST)/SwipePrettyPrinter.o

all: releasedist

clean: clean_releasedist

before_releasedist: 
	test -d bin/Release || mkdir -p bin/Release
	test -d $(OBJDIR_RELEASEDIST) || mkdir -p $(OBJDIR_RELEASEDIST)

after_releasedist: 

releasedist: before_releasedist out_releasedist after_releasedist

out_releasedist: before_releasedist $(OBJ_RELEASEDIST) $(DEP_RELEASEDIST)
	$(LD) $(LIBDIR_RELEASEDIST) -o $(OUT_RELEASEDIST) $(OBJ_RELEASEDIST)  $(LDFLAGS_RELEASEDIST) $(LIB_RELEASEDIST)

$(OBJDIR_RELEASEDIST)/Main.o: Main.cpp
	$(CXX) $(CFLAGS_RELEASEDIST) $(INC_RELEASEDIST) -c Main.cpp -o $(OBJDIR_RELEASEDIST)/Main.o

$(OBJDIR_RELEASEDIST)/SwipeCmdPrompt.o: SwipeCmdPrompt.cpp
	$(CXX) $(CFLAGS_RELEASEDIST) $(INC_RELEASEDIST) -c SwipeCmdPrompt.cpp -o $(OBJDIR_RELEASEDIST)/SwipeCmdPrompt.o

