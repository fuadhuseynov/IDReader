	$(CXX) $(CFLAGS_RELEASEDIST) $(INC_RELEASEDIST) -c SwipePrettyPrinter.cpp -o $(OBJDIR_RELEASEDIST)/SwipePrettyPrinter.o

clean_releasedist: 
	rm -f $(OBJ_RELEASEDIST) $(OUT_RELEASEDIST)
	rm -rf bin/Release
	rm -rf $(OBJDIR_RELEASEDIST)

.PHONY: before_releasedist after_releasedist clean_releasedist

